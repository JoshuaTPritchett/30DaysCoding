package server;

import java.net.ServerSocket;
/**
 * 
 * There where that server code gonna go
 * @author j
 *
 */
public abstract class Server {
	private int portNumber;
	ServerSocket serverSocket;
	public Server(int portN) {
		portNumber = portN;
		
	}
	
	abstract boolean serverStart() {
		return true;
		
	}
	
	
	
}
